<div class="page-header">
  <%= link_to bands_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_band_path(@band), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show band</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @band.name %></dd>

  <dt>Signed on:</dt>
  <dd><%= @band.signed_on %></dd>

  <dt>Agent:</dt>
  <dd><%= @band.agent %></dd>

</dl>
