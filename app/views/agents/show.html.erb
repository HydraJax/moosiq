<div class="page-header">
  <%= link_to agents_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_agent_path(@agent), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show agent</h1>
</div>

<dl class="dl-horizontal">
  <dt>First name:</dt>
  <dd><%= @agent.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @agent.last_name %></dd>

  <dt>Phone number:</dt>
  <dd><%= @agent.phone_number %></dd>

</dl>
